        $('.story-desc p a').css("color", "#0000FF");
        //AddRelatedNews();
        //RemoveEmptyPara();
    });

    function AddRelatedNews() {
        var news = $($('.news-wrap-block')[0]).next().text().trim();
        var relatedNews = new Array();



        if (false == true) {
            $.ajax({
                type: "POST",
                url: "/Home/RelatedNews",
                data: JSON.stringify(relatedNews),
                async: false,
                contentType: 'application/json; charset=utf-8',

                success: function(response) {

                    var pLength = $('.story-desc').children('p').length;
                    if (pLength> 4) {
                        $('.story-desc').children('p:eq(3)').append(response);
                    } else {
                        pLength = pLength - 1;
                        $('.story-desc').children('p:eq(' + parseInt(pLength) + ')').append(response);
                    }

                },
                error: function(xhr, error) {

                }
            });
        }
    }

</script>
