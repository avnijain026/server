        var isbold = $('.story-desc p:not(.p-ignore):lt(2)').find("strong").length;
        var src = $('#hdnSrc').val();
        if (isbold == 0 && (src == "" || src.toLowerCase() != "print")) {
            $(".p-ignore").show();
        }

        BoldPrintNews();


        //Ads placement
        PlaceAds();
        let newsId = $('#news-id').val();
        if ($(".top-img").attr("src") != undefined && newsId !='556751')
        {
            $(".img-container-detail").css({
                "background": "linear-gradient(0deg,rgba(255,255,255,0.5),rgba(255,255,255,0.5)),url(" + $(".top-img").attr("src").replace(" ", "%20") + ")",
                "background-size": "cover"
            });
